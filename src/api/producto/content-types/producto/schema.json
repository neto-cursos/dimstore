      "type": "string"
    },
    "prod_modelo": {
      "type": "string"
    },
    "prod_ancho": {
      "type": "float"
    },
    "prod_alto": {
      "type": "float"
    },
    "prod_profundidad": {
      "type": "float"
    },
    "prod_estado": {
      "type": "boolean"
    },
    "prod_descripcion": {
